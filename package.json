  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:core": "vitest run src/terminal/__tests__/ArgumentParser.test.js src/terminal/__tests__/formatters.test.js src/terminal/__tests__/registry.test.js",
