    "test:commands": "vitest run src/terminal/__tests__/commands.test.js src/terminal/__tests__/echo.test.js",
    "test:all": "vitest run",
    "format:check": "prettier --check .",
    "audit": "tsx scripts/audit.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "clean": "npx depcheck && npx npm-check-updates",
    "precommit": "npm-run-all lint:fix format typecheck test:all build",
    "fix": "npm-run-all lint format clean"
  },
  "dependencies": {
